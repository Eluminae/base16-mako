# You can use these variables anywhere in the mako configuration file.

[urgency=low]
    background-color = "#e8e4cf"
    text-color = "#999580"
    border-color = "#a6a28c"

[urgency=normal]
    background-color = "#a6a28c"
    text-color = "#6e6b5e"
    border-color = "#6e6b5e"

[urgency=high]
    background-color = "#d73737"
    text-color = "#292824"
    border-color = "#20201d"
